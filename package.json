{
  "name": "scoop",
  "version": "0.1.0",
  "type": "module",
  "description": "Inline prediction trading Chrome extension",
  "private": true,
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "icons": "node scripts/generate-icons.mjs",
    "prebuild": "npm run icons",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "bot": "tsx bot/bot.ts",
    "bot:dry": "tsx bot/bot.ts --dry-run",
    "bot:mock": "tsx bot/bot.ts --mock --dry-run"
  },
  "dependencies": {
    "ethers": "^6.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.279",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.3.1",
    "eslint": "^9.17.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "twitter-api-v2": "^1.29.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-static-copy": "^2.2.0"
  }
}
